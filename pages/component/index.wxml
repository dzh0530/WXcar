<view class="broadcast cf" style="padding-bottom:10px;border-bottom:8px solid #F3F1F1;">
        <view bindtap="refreshList" class="moro25" data-type="1">
            <image src="resources/pic/111.png"></image>
            <text>人找车</text>
        </view>
        <view bindtap="refreshList" class="moro25" data-type="2">
            <image src="resources/pic/222.png"></image>
            <text>车找人</text>
        </view>
        <view bindtap="refreshList" class="moro25" data-type="4">
            <image src="resources/pic/333.png"></image>
            <text>车找货</text>
        </view>
        <view bindtap="refreshList" class="moro25" data-type="3">
            <image src="resources/pic/444.png"></image>
            <text>货找车</text>
        </view>
    </view>


 <view class="box">

    <view class="list-bd">
      <block wx:for="{{list}}">
      <view id="{{item.id}}" data-type="{{item.type}}" bindtap="goDetail"  class="item has-hairline hairline-in-pseudo-bottom">
        <view class="list-tags" style="padding-bottom:10px;border-bottom:2px solid white;">
     
          <text class="tag shown-top" wx:if="{{item.istop}}">置顶 </text>
          <text class="tag bg-find-car" wx:if="{{item.type === 1}}">人找车</text>
          <text class="tag bg-find-people" wx:if="{{item.type === 2}}">车找人</text>
           <text class="tag bg-find-car"  style='background:#45C2FB' wx:if="{{item.type === 4}}">车找货</text>
            <text class="tag bg-find-car"  style='background:#DF81F6' wx:if="{{item.type === 3}}">货找车</text>
   
         <text class="tag num" >    <text wx:if="{{item.type === 3}}">货重</text><text wx:if="{{item.type === 4}}">载重</text><text wx:if="{{item.type === 1}}">有</text><block wx:if="{{item.type === 2}}">  <text  wx:if = "{{item.user_count == 0}}"></text>
              <text wx:else >还有</text></block>
              <text style='color:red'  wx:if="{{item.type === 1}}"> {{item.user_count}}</text>
                  <text style='color:red'  wx:if="{{item.type === 2}}"> {{item.user_count}}</text>
              <text wx:if="{{item.type === 1}}">人同行</text>
               <block wx:if="{{item.type === 2}}">
              <text  wx:if = "{{item.user_count == 0}}">【已满座】</text>
              <text wx:else >个座位</text>
            </block>
              <text wx:if="{{item.type === 3}}">{{item.weight}}</text>
            <text wx:if="{{item.type === 4}}">{{item.weight}}</text>
            </text>
          <text class="tag-day1 today" >查看详情</text>
        
        </view>
          <view class="list-address">
          <text>{{item.from_place}}</text>  <image src="/pages/common/images/huang.png"  wx:if="{{item.type === 1}}"/>
         <image src="/pages/common/images/lv.png"  wx:if="{{item.type === 2}}"/>
          <image src="/pages/common/images/lan.png"  wx:if="{{item.type === 4}}"/>
           <image src="/pages/common/images/mei.png"  wx:if="{{item.type === 3}}"/>
          <text>{{item.to_place}}</text>
          
        </view>
        <view class="list-time"  >
          <view >  
       <text style='color:#ff8400'  wx:if="{{item.daytype === 1}}">今天</text>
          <text  style='color:#ff8400'  wx:if="{{item.daytype === 2}}">明天</text>
          出发：{{item.start_time}} 
           
         
         
       </view>    
        </view>
        <view class="list-sub">
         

          
          <text class="note" style='width:92%' wx:if="{{item.note}}">备注：{{item.note}}</text>
         
        </view>
      </view><!-- item end --> <button  id="{{item.id}}" bindtap="makeCall" style='margin-top:-90rpx ; margin-right:50rpx;width:45px;height:30px;  float:right; border:1px solid #ff8400;border-radius: 15%; color:#fff; background:#FF7F00 url(data:image/png;base64,) no-repeat 14px 6px; background-size: 14px 16px;  text-align:left; text-indent: 14px; '></button>


      <button  id="{{item.id}}" bindtap="makeCall1" style='margin-top:-90rpx ; margin-right:150rpx;width:45px;height:30px;  float:right; border:1px solid #17a976;border-radius: 15%; background-color:grey;background:white url(data:image/png;base64,) no-repeat 14px 6px; background-size: 17px 18px;  text-align:left; text-indent: 15px;  '> </button>
      </block>
    </view>
  </view>


