<view class="page">
  <view class="search" style=' float: left;height:90rpx;padding:10rpx;background:white;opacity:{{0.95}};position: fixed;z-index: 990;width:100%'>
      <input type='text' bindinput="onFromPlace" placeholder="从哪儿出发" placeholder-class="phcolor"/>
      <image src="resources/pic/car7.png"></image>
      <input type='text' bindinput="onToPlace" placeholder="要到哪儿去" placeholder-class="phcolor"/>
      <button bindtap="goFilterInfo"  bindtap="filterNow" class="search_btn"  >查询</button>
  </view>
  <view style='height:60px'></view>
  <view class="prcode">
    <view class="prcode-box">
    <text class='prcode-box-close'>×</text>
    <text class='prcode-box-text'>长按识别二维码</text>
    <image class="prcode-box-img" bindtap="previewImage" src="resources/pic/service.jpg"></image>
    </view>
  </view>
  <view class="share-box">
    <image class="share-img" src="{{sharePic}}" bindload="shareImageLoad" style="height:{{shareImageHeight}}px"></image>
  </view>

  <!-- <view class="banner" bindtap="tapBanner">
    <image class="banner-img" src="{{bannerImgSrc}}"></image>
  </view> -->


  
  <swiper style='height:120px'  indicator-dots="{{indicatorDots}}"  
          autoplay="{{autoplay}}" interval="{{interval}}" duration="{{duration}}">  
        <block wx:for="{{imgUrls}}">  
          <swiper-item>  
            <navigator hover-class="navigator-hover">  
              <image style='width:100%;height:120px' src="{{item.url}}" class="slide-image"/>  
            </navigator>   
          </swiper-item>  
        </block>  
  </swiper> 
  <view class="broadcast cf" style="padding-bottom:10px;border-bottom:8px solid #F3F1F1;">
    <view class="broadcast-icon"></view>
    <view class="broadcast-title" bindtap="tapBroadcastTitle" >
      <text>{{bannerTitle}}</text>
    
     
    </view>
  </view>



   
